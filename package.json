{
  "name": "portafolio-personal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "watch:css": "node-sass sass/style.scss styles/App.module.css -w",
    "compile:css": "node-sass sass/style.scss styles/App.comp.css",
    "concat:css": "concat -o styles/App.concat.css styles/App.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 version\" styles/App.concat.css -o styles/App.prefix.css",
    "compress:css": "node-sass styles/App.prefix.css styles/App.module.css --output-style compressed",
    "build:css": "npm-run-all compile:css concat:css prefix:css compress:css"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "canvas-sketch-util": "^1.10.0",
    "concat": "^1.0.3",
    "next": "12.2.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swiper": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3"
  }
}
